package beetle.netpackage;

import java.io.DataInputStream;
import java.io.InputStream;

public interface IDataReader {
	DataInputStream GetStream();
	int Read(byte[] data);
	int Read(byte[] data,int off,int len);
	boolean ReadBoolean();
	byte ReadByte();
	char ReadChar();
	double ReadDouble() ;
	float ReadFloat();
	int ReadInt();
	short ReadShort();
	long ReadLong();
	String ReadUTF();
	void Read(IMessage msg);
}
