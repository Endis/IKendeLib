package beetle.netpackage;

public interface IDataReader {
	int Read(byte[] data);
	int Read(byte[] data,int off,int len);
	boolean ReadBoolean();
	byte ReadByte();
	char ReadChar();
	double ReadDouble() ;
	float ReadFloat();
	int ReadInt();
	short ReadShort();
	long ReadLong();
	String ReadUTF();
}
